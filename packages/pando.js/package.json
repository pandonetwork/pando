{
  "name": "@pando/pando.js",
  "version": "1.0.0",
  "description": "Typescript implementation of pando",
  "author": "Ryhope Network <contact@ryhope.network>",
  "contributors": [
    "Olivier Sarrouy <olivier@ryhope.network>"
  ],
  "license": "(GPL-3.0-or-later OR MIT)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryhope/pando"
  },
  "keywords": [
    "vcs",
    "git",
    "pando",
    "ipfs",
    "ipld",
    "ethereum",
    "aragon",
    "ryhope"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "node_modules/.bin/_mocha --require babel-core/register --timeout 100000 test/global.js",
    "coverage": "nyc npm test",
    "lint": "npm run lint:base && npm run lint:test",
    "lint:base": "./node_modules/.bin/tslint --fix src/**/*.ts{,x}",
    "lint:test": "./node_modules/.bin/eslint --fix test/**/*.js"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "./lib/**/*.js"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "_moduleAliases": {
    "@components": "./lib/components",
    "@factories": "./lib/factories",
    "@interfaces": "./lib/interfaces",
    "@objects": "./lib/objects/",
    "@root": "./lib/pando/",
    "@utils": "./lib/utils"
  },
  "devDependencies": {
    "@0xproject/types": "^0.5.0",
    "@types/node": "^9.6.23",
    "async": "^2.6.1",
    "awesome-typescript-loader": "^5.2.0",
    "babel-eslint": "^8.2.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "intern": "^4.2.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.1.0",
    "nyc": "^11.6.0",
    "remap-istanbul": "^0.11.1",
    "source-map-support": "^0.5.6",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.9.2",
    "webpack": "^4.15.1",
    "webpack-cli": "^2.0.14",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@aragon/os": "^3.1.3",
    "@pando/core": "file:../core",
    "cids": "^0.5.3",
    "eth-ens-namehash": "^2.0.8",
    "ethereum-regex": "^1.1.3",
    "ipfs-api": "^22.2.2",
    "ipld-pando": "file:../js-ipld-pando",
    "is-ipfs": "^0.3.2",
    "js-sha3": "^0.7.0",
    "js-yaml": "^3.11.0",
    "jsonfile": "^4.0.0",
    "klaw-sync": "^3.0.2",
    "lodash": "^4.17.10",
    "module-alias": "^2.1.0",
    "promisify-event": "^1.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "truffle-contract": "^3.0.6",
    "truffle-hdwallet-provider": "0.0.5",
    "url-parse": "^1.4.1",
    "web3": "^1.0.0-beta.34",
    "web3-utils": "^1.0.0-beta.34"
  }
}
