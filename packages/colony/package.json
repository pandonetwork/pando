	"author": "Pando Network <contact@pando.network>",
	"contributors": [
		"Olivier Sarrouy <olivier@pando.network>",
		"Deam Hansen <deam@pando.network>"
	],
	"repository": "github:pandonetwork/pando",
	"homepage": "https://github.com/pandonetwork/pando",
	"bugs": "https://github.com/pandonetwork/pando/issues",
	"license": "(GPL-3.0-or-later OR MIT)",
	"keywords": [
		"pando",
		"vcs",
		"dao",
		"ipfs",
		"ethereum",
		"aragon"
	],
	"dependencies": {
		"@aragon/apps-shared-minime": "^1.0.0",
		"@aragon/client": "^1.0.0-beta.9",
		"@aragon/os": "4.0.1",
		"@aragon/ui": "^0.27.0",
		"@pando/core": "^0.0.1",
		"@pando/organism": "^0.0.1",
		"@pando/scheme-democracy": "^0.0.1",
		"ipfs-http-client": "^29.0.0",
		"react": "^16.7.0-alpha.2",
		"react-dom": "^16.7.0-alpha.2",
		"styled-components": "^3.4.9",
		"styled-system": "^3.2.0",
		"web3-utils": "^1.0.0-beta.37"
	},
	"devDependencies": {
		"@aragon/apps-finance": "^2.1.0",
		"@aragon/apps-token-manager": "^2.0.0",
		"@aragon/apps-vault": "^4.0.0",
		"@aragon/apps-voting": "^2.0.0",
		"@aragon/cli": "5.2.1",
		"@aragon/test-helpers": "^1.0.1",
		"@babel/core": "^7.1.2",
		"@babel/plugin-proposal-class-properties": "^7.1.0",
		"@babel/polyfill": "^7.0.0",
		"@babel/preset-env": "^7.1.0",
		"@pando/factory": "^0.0.1",
		"@pando/helpers": "^0.0.1",
		"babel-eslint": "^10.0.1",
		"babel-plugin-styled-components": "^1.8.0",
		"chai": "^4.2.0",
		"chai-as-promised": "^7.1.1",
		"eslint": "^5.12.0",
		"eslint-config-prettier": "^3.3.0",
		"eslint-config-standard": "^12.0.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-node": "^8.0.1",
		"eslint-plugin-prettier": "^3.0.1",
		"eslint-plugin-promise": "^4.0.1",
		"eslint-plugin-react": "^7.12.3",
		"eslint-plugin-standard": "^4.0.0",
		"ethlint": "^1.2.1",
		"parcel-bundler": "^1.10.1",
		"prettier": "^1.15.3",
		"truffle": "5.0.0-beta.2"
	},
	"scripts": {
		"lint:solidity": "solium --dir ./contracts --fix",
		"lint:js": "eslint test",
		"lint": "npm run lint:solidity && npm run lint:js",
		"compile": "aragon contracts compile",
		"start:aragon:ipfs": "aragon run --kit OrganizationFactory --kit-init @ARAGON_ENS --environment default",
		"start:aragon:http": "aragon run --kit OrganizationFactory --kit-init @ARAGON_ENS --http localhost:8001 --http-served-from ./dist",
		"start:app": "npm run build:sync && npm run build:script -- --no-minify && parcel serve app/index.html -p 8001 --out-dir dist/ --no-cache",
		"start": "npm run start:aragon:ipfs",
		"build:sync": "copy-aragon-ui-assets -n aragon-ui ./dist",
		"build:app": "parcel build app/index.html -d dist/ --public-url '.' --no-cache",
		"build:script": "parcel build app/script.js -d dist/ --no-cache",
		"build": "npm run build:sync && npm run build:app && npm run build:script",
		"test": "aragon contracts test --network devnet",
		"publish:devnet": "aragon apm publish patch --environment default",
		"publish": "npm run publish:devnet"
	}
}
