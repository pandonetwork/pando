{
  "name": "pando-repository",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint ./src",
    "sync-assets": "copy-aragon-ui-assets -n aragon-ui ./build && rsync -rtu ./public/ ./build",
    "start": "npm run sync-assets && npm run build:script -- --no-minify && parcel serve index.html -p 3001 --out-dir build/",
    "build": "npm run sync-assets && npm run build:script && parcel build index.html --out-dir build/ --public-url '.'",
    "build:script": "parcel build src/script.js --out-dir build/"
  },
  "dependencies": {
    "@aragon/api": "^1.0.0",
    "@aragon/ui": "^0.32.0",
    "@githubprimer/octicons-react": "^8.5.0",
    "cids": "^0.5.7",
    "codemirror": "^5.47.0",
    "ipfs-http-client": "^29.1.1",
    "ipld": "^0.21.1",
    "ipld-git": "^0.2.3",
    "lodash.orderby": "^4.6.0",
    "lodash.uniqwith": "^4.5.0",
    "prismjs": "^1.16.0",
    "prop-types": "^15.6.0",
    "react": "^16.8.5",
    "react-codemirror2": "^6.0.0",
    "react-diff-viewer": "^1.0.4",
    "react-dom": "^16.7.0-alpha.2",
    "react-markdown": "^4.0.6",
    "rxjs": "^6.4.0",
    "styled-components": "4.1.3",
    "styled-system": "^3.2.1",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "5.5.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^4.0.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.8.2"
  },
  "browserslist": [
    ">2%",
    "last 1 edge versions",
    "not ie > 0",
    "not op_mini all"
  ]
}
